void setup() {
  Serial.begin(9600);
  pinMode(LEFT_BUTTON_PIN, INPUT_PULLUP);
  pinMode(RIGHT_BUTTON_PIN, INPUT_PULLUP);

  if(!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) { 
    Serial.println(F("SSD1306 allocation failed"));
    for(;;); 
  }

}

void loop() {
  display.clearDisplay();

  if (digitalRead(LEFT_BUTTON_PIN) == LOW && digitalRead(RIGHT_BUTTON_PIN) == LOW){
    display.clearDisplay();
    display.drawBitmap(0, 0, double_tap, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
  }
  else if (digitalRead(LEFT_BUTTON_PIN) == LOW && digitalRead(RIGHT_BUTTON_PIN) == HIGH){
    display.clearDisplay();
    display.drawBitmap(0, 0, tap_left, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
  }
  else if (digitalRead(LEFT_BUTTON_PIN) == HIGH && digitalRead(RIGHT_BUTTON_PIN) == LOW){
    display.clearDisplay();
    display.drawBitmap(0, 0, tap_right, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
  }
  else{
    display.clearDisplay();
    display.drawBitmap(0, 0, base, SCREEN_WIDTH, SCREEN_HEIGHT, WHITE);
  }
  display.display();
}